{
  "name": "ht-flex-api-server",
  "version": "1.0.0",
  "description": "Api Server for Hello Tractor. Postgrel Node and Express",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "jest --coverage --verbose"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.7",
    "@types/swagger-ui-express": "^3.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^5.0.0-alpha.7",
    "kinvey-flex-sdk": "^3.3.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "swagger-ui-express": "^4.1.1",
    "tsc-watch": "^4.0.0",
    "typeorm": "^0.2.19",
    "typescript": "^3.6.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/pg": "^7.11.2",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0"
  }
}
